<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="checkout_item_price_renderers"/>
    <body>
        <referenceBlock name="quotemaster.quote.item.renderers">
            <block class="Magento\Checkout\Block\Cart\Item\Renderer" name="quotemaster.quote.item.renderers.default" as="default" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.default.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.default.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.default.actions.remove" template="QuoteMaster_QuoteStorm::quote/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\Checkout\Block\Cart\Item\Renderer" name="quotemaster.quote.item.renderers.simple" as="simple" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.simple.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.simple.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.simple.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\Downloadable\Block\Checkout\Cart\Item\Renderer" name="quote.cart.item.renderers.downloadable" as="downloadable" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.downloadable.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.downloadable.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.downloadable.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\ConfigurableProduct\Block\Cart\Item\Renderer\Configurable" name="quote.item.renderer.configurable" as="configurable" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.configurable.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.configurable.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.configurable.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\Bundle\Block\Checkout\Cart\Item\Renderer" name="quote.item.renderer.bundle" as="bundle" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.bundle.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.bundle.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.bundle.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\GroupedProduct\Block\Cart\Item\Renderer\Grouped" name="quote.item.renderer.grouped" as="grouped" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.grouped.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.grouped.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.grouped.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
            <block class="Magento\Checkout\Block\Cart\Item\Renderer" name="quote.item.renderer.virtual" as="virtual" template="QuoteMaster_QuoteStorm::quote/item/default.phtml">
                <block class="Magento\Checkout\Block\Cart\Item\Renderer\Actions" name="quotemaster.quote.item.renderers.virtual.actions" as="actions">
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Edit" name="quotemaster.quote.item.renderers.virtual.actions.edit" template="Magento_Checkout::cart/item/renderer/actions/edit.phtml"/>
                    <block class="QuoteMaster\QuoteStorm\Block\Quote\Item\Renderer\Actions\Remove" name="quotemaster.quote.item.renderers.virtual.actions.remove" template="QuoteMaster_QuoteStorm::quote/item/renderer/actions/remove.phtml"/>
                </block>
            </block>
        </referenceBlock>
    </body>
</page>
